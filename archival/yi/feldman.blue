      <instrument type='blue.orchestra.BlueSynthBuilder' editEnabled='true'>
        <name>b64 - Orch</name>
        <comment>Inspired by the SID sound chip commonly used in Commodore 64 computers, 
using simliar waveforms and options for ring modulation.</comment>
        <globalOrc>
</globalOrc>
        <globalSco>


</globalSco>
        <instrumentText>;p3 = p3 + &lt;release&gt;

ipch 	getFrequency p4
ipch2 	getFrequency p5

kpchline 	line ipch, p3, ipch2

iamp 	= ampdb(p6)
iSpace	= p7
ienvType	= p8

kenv	yiEnvelope ienvType, &lt;attack&gt;, &lt;decay&gt;, &lt;sustain&gt;, &lt;release&gt;

aout1 	multiOscil &lt;waveform1&gt;, kpchline * &lt;freqMultiply1&gt;, &lt;pulseWidth1&gt;
aout2 	multiOscil &lt;waveform2&gt;, kpchline * &lt;freqMultiply2&gt;, &lt;pulseWidth2&gt;
aout3 	multiOscil &lt;waveform3&gt;, kpchline * &lt;freqMultiply3&gt;, &lt;pulseWidth3&gt;

aout1 	simpleMultiFilter	aout1 * &lt;amp1&gt;, &lt;filterType1&gt;, &lt;filterFreq1&gt;, &lt;filterBandwidth1&gt;
aout2 	simpleMultiFilter	aout2 * &lt;amp2&gt;, &lt;filterType2&gt;, &lt;filterFreq2&gt;, &lt;filterBandwidth2&gt;
aout3 	simpleMultiFilter	aout3 * &lt;amp3&gt;, &lt;filterType3&gt;, &lt;filterFreq3&gt;, &lt;filterBandwidth3&gt;

aout = 0

if (&lt;ringmod1&gt; == 1) then
	atemp = aout1 * aout3
	aout = atemp
else
	aout = aout1
endif

if (&lt;ringmod2&gt; == 1) then
	atemp = aout2 * aout1
	aout = aout + atemp
else
	aout = aout + aout2
endif


if (&lt;ringmod3&gt; == 1) then
	atemp = aout3 * aout2
	aout = aout + atemp
else
	aout = aout + aout3
endif


;aout butterlp aout, 8000
;aout butterhp aout, 500
;aout butterbp aout, 500, 1000

aout = aout * kenv * iamp

blueMixerOut aout, aout</instrumentText>
